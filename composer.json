{
  "name": "arne-groskurth/temp-file",
  "description": "TempFile is a small library inspired by the SplTempFileObject providing solutions for commonly occurring tasks when dealing with temporary files.",
  "keywords": ["temp", "file", "splfileobject"],
  "homepage": "https://github.com/arnegroskurth/TempFile",
  "authors": [
    {
      "name": "Arne Groskurth",
      "email": "arne.groskurth@gears9.de"
    }
  ],
  "license": "MIT",
  "require": {
    "ext-fileinfo": "*",
    "php": "^5.4 || ^7.0 || ^8.0",
    "symfony/http-foundation": "^3.1 || ^4.0 || ^5.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^5.5 || ^9.3.0"
  },
  "scripts": {
    "test": "phpunit --colors=always --verbose"
  },
  "autoload": {
    "psr-4": {
      "ArneGroskurth\\TempFile\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArneGroskurth\\TempFile\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
}
